.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #000;
  font-weight: normal;
  font-size: 14px;
}


.page-body {
  padding-top: 70px;
  background-color: #fff;
}

.weui-cell {
  border-bottom: 1px solid #eee;
  border-top: 1px solid #eee;
}

